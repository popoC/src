<!-- usb_cam_opencv.launch-->
<launch>

  <!-- image overlay node -->
  <node pkg="opencv_po_tx1" type="demo_opencv_overlay" name="Graphical_User_Interface_1" output="screen">
  <param name="video_node" value="/camera_array/cam0/image_raw" />
  </node>

 <!-- USB camera 2 
  <node pkg="usb_cam" type="usb_cam_node" name="usb_cam_2">
  <param name="video_device" value="/dev/video2" />
  <param name="image_width" value = "1280"   />
  <param name="image_height" value = "720"   />
  <param name="pixel_format" value = "mjpeg"   />
  <param name="io_method" value="mmap"/>
  </node>
  <!- - image overlay node 
  <node pkg="opencv_po_tx1" type="opencv_edge_node_tx1" name="Graphical_User_Interface_2" output="screen">
  </node>  -->
</launch>
